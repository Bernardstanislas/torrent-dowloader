{
  "manifest_version": 2,

  "name": "Torrent downloader",
  "description": "This extension will help downloading torrents",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab"
   ]
}
